{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "watch": {
      "commandName": "Executable",
      "workingDirectory": "$(ProjectDir)",
      "executablePath": "dotnet",
      "commandLineArgs": "watch run debug --launch-profile development"
    },
    "development": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "hotReloadEnabled": true,
      "launchBrowser": false,
      "launchUrl": "swagger",
      "applicationUrl": "http://0.0.0.0:7000;https://0.0.0.0:7001",
      "environmentVariables": {
        "WEB_APP__DOMAIN": "https://localhost:5001",
        "ASPNETCORE_HOSTINGSTARTUPASSEMBLIES": "Domain;Application;Infrastructure;Presentation",
        "ASPNETCORE_ENVIRONMENT": "Development",
        "JWT__KEY": "66F475922093498BA388EF954EF6D65E",
        "JWT__ISSUER": "localhost",
        "JWT__AUDIENCE": "localhost",
        "JWT__EXPIRATION": "60"
      }
    }
  }
}
